<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>Space</th>
      <th>Venue ID</th>
      <th>Privacy</th>
      <th>Seated</th>
      <th>Standing</th>
    </tr>

    <% @spaces.each do |space| %>
      <tr>
        <td><%= space.name %></td>
        <td><%= space.venue__c %></td>
        <td><%= space.privacy__c %></td>
        <td><%= space.seated__c %></td>
        <td><%= space.standing__c %></td>
      </tr>
    <% end %>
  </table>

</div>
